{
  "name": "key-viewer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "electron": "8",
    "electron-builder": "^22.10.5",
    "electronmon": "^1.1.2",
    "node-abi": "^2.19.1"
  },
  "dependencies": {
    "iohook": "^0.6.6",
    "lodash": "^4.17.20"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "4.0.0"
  },
  "iohook": {
    "targets": [
      "node-72",
      "electron-76"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "productName": "KeyViewer",
    "appId": "com.github.pikokr.keyviewer",
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "./app/",
      "app": "."
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "appimage"
      ],
      "category": "Utility"
    },
    "asarUnpack": [
      "config.json"
    ]
  }
}
